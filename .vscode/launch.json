    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug App",
            "request": "launch",
            "type": "node",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [
                "${workspaceFolder}/src/app/Launcher.ts"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "JS Debug",
            "program": "${workspaceFolder}/dist/app/Launcher.js",
        }
    ]
}